extend type Query {
    showFlows(project: String): [FlowView]
    showDeployedFlows: [DeployedFlowView]
    showProjects: [String]
}

type FlowView {
    projectName: String
    name: String
    type: String
    template: String
}

type DeployedFlowView {
    appName: String
    flowName: String
    deployTime: Date
}